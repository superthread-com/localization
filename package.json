{
  "name": "@superthread-com/localization",
  "version": "1.0.7",
  "description": "Superthread front-end localizations",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/superthread-com/internationalization.git",
    "directory": "packages/localization"
  },
  "scripts": {
    "clean": "rm -rf dist node_modules",
    "build": "node build.mjs",
    "compileEn": "tsc --esModuleInterop --module commonjs ./src/en/index.ts --outDir ./temp && mv ./temp/en/index.js ./temp/en/index.cjs",
    "makeEnum": "npm run compileEn && node makeEnum.mjs && npm run declarations",
    "postmakeEnum": "rimraf ./temp",
    "declarations": "tsc src/index.ts --declaration --emitDeclarationOnly --outDir dist",
    "test": "vitest run",
    "prepublish": "npm run makeEnum && npm run build"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^2.0.1",
    "chai": "^5.1.1",
    "esbuild": "^0.23.0",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "vitest": "^2.0.1"
  },
  "author": "@superthread-com",
  "license": "ISC"
}
